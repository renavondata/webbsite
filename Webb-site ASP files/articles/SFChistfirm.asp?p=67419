		Activity type: <select name='a' onchange="this.form.submit()"><option value='0' selected>All</option><option value='6'>Advising on corporate finance</option><option value='5'>Advising on futures contracts</option><option value='4'>Advising on securities</option><option value='9'>Asset management</option><option value='7'>Automated trading services</option><option value='12'>Client clearing services for OTC derivative transactions</option><option value='10'>Credit rating services</option><option value='2'>Dealing in futures contracts</option><option value='11'>Dealing in or advising on OTC derivative products</option><option value='1'>Dealing in securities</option><option value='13'>Depositary services for relevant CISs</option><option value='3'>Leveraged foreign exchange trading</option><option value='8'>Securities margin financing</option></select>
